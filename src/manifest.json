{
  "name": "__MSG_appTitle__",
  "manifest_version": 3,
  "version": "0.0.1",
  "description": "__MSG_appDescription__",
  "permissions": [
    "contextMenus",
    "bookmarks",
    "tabs",
    "storage",
    "unlimitedStorage",
    "activeTab"
  ],
  "background": {
    "service_worker": "./background/index.js",
    "type": "module"
  },
  "action": {
    "default_title": "__MSG_actionTitle__",
    "default_popup": "./popup.html",
    "default_icon": {
      "16": "./assets/images/logo_16.png",
      "32": "./assets/images/logo_32.png",
      "48": "./assets/images/logo_48.png",
      "64": "./assets/images/logo_64.png",
      "128": "./assets/images/logo_128.png"
    }
  },
  "options_ui": "./options.html",
  "chrome_url_overrides": {
    "newtab": "./newTab.html"
  },
  "icons": {
    "16": "./assets/images/logo_16.png",
    "32": "./assets/images/logo_32.png",
    "48": "./assets/images/logo_48.png",
    "64": "./assets/images/logo_64.png",
    "128": "./assets/images/logo_128.png"
  },
  "default_locale": "en",
  "commands": {
    "run-save": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y",
        "mac": "Command+Shift+Y"
      },
      "description": "Run \"foo\" on the current page."
    },
    "_execute_action": {
      "suggested_key": {
        "windows": "Ctrl+Shift+Y",
        "mac": "Command+Shift+Y",
        "chromeos": "Ctrl+Shift+U",
        "linux": "Ctrl+Shift+J"
      }
    }
  },
  "homepage_url": "https://github.com/helinyu/LightNote"
}
